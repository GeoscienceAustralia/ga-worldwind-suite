<?xml version="1.0" encoding="UTF-8"?>
<!-- 
	A complete Fog layer definition. All fields included.
 -->
<Layer version="1" layerType="FogLayer">
	<!-- The display name. Optional, but recommended. -->
	<DisplayName>testName</DisplayName>
	
	<!-- The near fog factor. Optional. Defaults to 1.1 -->
	<NearFactor>1.2</NearFactor>
	
	<!-- The far fog factor. Optional. Defaults to 1.1 -->
	<FarFactor>1.3</FarFactor>
	
	<!-- The fog color. Optional. -->
	<FogColor red="123" green="23" blue="222" />
</Layer>